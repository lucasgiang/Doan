@model IEnumerable<Doan.Models.LOAISP>

@{
    ViewBag.Title = "Loaisanpham";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Loaisanpham</h2>

<p>
    @Html.ActionLink("Create New", "Themmoilsp")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TENLOAISP)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TENLOAISP)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Sualsp", new { id = item.MALOAISP }) |
                @Html.ActionLink("Chi tiết", "Chitietlsp", new { id = item.MALOAISP }) |
                @Html.ActionLink("Xóa", "Xoalsp", new { id = item.MALOAISP })
            </td>
        </tr>
    }

</table>
